<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.VBox?>
	 <VBox prefHeight="20.0" prefWidth="600.0"  xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Menu">
	  <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Ações">
          <items>
            <MenuItem mnemonicParsing="false" text="Novo"/>
             <Menu mnemonicParsing="false" text="Objetos de Dados" >
             <MenuItem mnemonicParsing="false" text="Coluna" />
             <MenuItem mnemonicParsing="false" text="Sequence" />
             <MenuItem mnemonicParsing="false" text="Tabela" />
            </Menu>
             <Menu mnemonicParsing="false" text="Objetos de Teste" >
             <MenuItem mnemonicParsing="false" text="Produtos Suprimentos" onAction="#goProdutoSupri"/>
             </Menu>
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Exportar" />
            <MenuItem mnemonicParsing="false" text="Save" />
            <MenuItem mnemonicParsing="false" text="Save As…" />
            <MenuItem mnemonicParsing="false" text="Revert" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Integrações">
          <items>
           <Menu mnemonicParsing="false" text="Jira" >
             <MenuItem mnemonicParsing="false" text="Chamados" onAction="#goChamado"/>
             <MenuItem mnemonicParsing="false" text="Comentários" onAction="#goOrientacaoTicket"/>
             <MenuItem mnemonicParsing="false" text="Apontamento" onAction="#goApontamento"/>
             </Menu>
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Configurações">
          <items>
           <Menu mnemonicParsing="false" text="Cadastros" >
            <MenuItem mnemonicParsing="false" text="Objetos" onAction="#goObjetos"/>
            <MenuItem mnemonicParsing="false" text="Prefixos" onAction="#goPrefixos"/>
            <MenuItem mnemonicParsing="false" text="Conexões" onAction="#goConexao"/>
            <MenuItem mnemonicParsing="false" text="Produtos" onAction="#goProdutos"/>
            <MenuItem mnemonicParsing="false" text="Versões" onAction="#goProdutos"/>
             <MenuItem mnemonicParsing="false" text="Orientações" onAction="#goOrientacaoPadrao"/>
             <MenuItem mnemonicParsing="false" text="Usuários" onAction="#goUsuarios"/>
            </Menu>
             <Menu mnemonicParsing="false" text="Suprimentos" >
                     <MenuItem mnemonicParsing="false" text="Configurações" onAction="#goConfigSupri"/>
             </Menu>
          </items>
        </Menu>
      </menus>
    </MenuBar>
</VBox>
