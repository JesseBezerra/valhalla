<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.layout.VBox?>
	 <VBox prefHeight="600.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Menu">
	  <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Ações">
          <items>
            <MenuItem mnemonicParsing="false" text="Novo"/>
             <Menu mnemonicParsing="false" text="Objetos de Dados" >
             <MenuItem mnemonicParsing="false" text="Coluna" />
             <MenuItem mnemonicParsing="false" text="Sequence" />
             <MenuItem mnemonicParsing="false" text="Tabela" />
            </Menu>
             <Menu mnemonicParsing="false" text="Objetos de Teste" >
             <MenuItem mnemonicParsing="false" text="Produtos Suprimentos" onAction="#goProdutoSupri"/>
             </Menu>
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Exportar" />
            <MenuItem mnemonicParsing="false" text="Save" />
            <MenuItem mnemonicParsing="false" text="Save As…" />
            <MenuItem mnemonicParsing="false" text="Revert" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" text="Undo" />
            <MenuItem mnemonicParsing="false" text="Redo" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Cut" />
            <MenuItem mnemonicParsing="false" text="Copy" />
            <MenuItem mnemonicParsing="false" text="Paste" />
            <MenuItem mnemonicParsing="false" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Select All" />
            <MenuItem mnemonicParsing="false" text="Unselect All" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Configurações">
          <items>
           <Menu mnemonicParsing="false" text="Cadastros" >
            <MenuItem mnemonicParsing="false" text="Cadastro de Objetos" onAction="#goObjetos"/>
            <MenuItem mnemonicParsing="false" text="Cadastro de Prefixos" onAction="#goPrefixos"/>
            <MenuItem mnemonicParsing="false" text="Cadastro de Conexões" onAction="#goConexao"/>
            <MenuItem mnemonicParsing="false" text="Cadastro de Produtos" onAction="#goProdutos"/>
            <MenuItem mnemonicParsing="false" text="Cadastro de Versões" onAction="#goProdutos"/>
            </Menu>
          </items>
        </Menu>
      </menus>
    </MenuBar>
</VBox>
